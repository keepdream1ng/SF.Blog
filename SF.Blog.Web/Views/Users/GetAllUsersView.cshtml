@model SF.Blog.Web.Views.Users.GetAllUsersViewModel
@{
}

@foreach (var user in Model.Users)
{
<div class="card col m-2">
	<div class="row">
		<div class="col card-text m-2">
			<p>
				@user.Email
				also known as
				@user.Name
			</p>
		</div>
		<a 
			class="col btn btn-primary m-2"
			role="button"
			href="@Url.Action("Edit", "Users", new{ id = user.Id })"
			>
			Edit / delete @user.Email
		</a>
	</div>
</div>
}
